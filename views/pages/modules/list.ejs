<% /** @var {[{code,title}]} modules  */ %>
<div class="modules-page">
  <div class="main container clearfix">
    <form action="" method="post">
      <select name="modules[]" id="modules" title="Select Zoho modules" multiple required size="<%= modules.length %>">
        <% modules.forEach(function(module){ %>
          <option value="<%= module.code %>"><%= module.title %></option>
        <% }); %>
      </select>
      <button>Submit</button>
    </form>
  </div>
</div>
